<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="HomePageAnchorPane" prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectFile.HomePageController">
   <children>
      <VBox fx:id="mainContainerVbox" alignment="CENTER" layoutX="50.0" layoutY="100.0" prefHeight="500.0" prefWidth="400.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#handleSandwichesMenu" prefHeight="35.0" prefWidth="300.0" text="Sandwiches">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#handleMealsMenu" prefHeight="35.0" prefWidth="300.0" text="Meals">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#handleDessertMenu" prefHeight="35.0" prefWidth="300.0" text="Dessert">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#handleDrinksMenu" prefHeight="35.0" prefWidth="300.0" text="Drinks" />
         </children>
      </VBox>
      <ToolBar layoutX="-2.0" layoutY="647.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="53.0" prefWidth="500.0">
        <items>
          <Button mnemonicParsing="false" onMouseClicked="#handleLogout" text="log out" />
            <Button mnemonicParsing="false" onMouseClicked="#handleMyOrders" text="my orders" />
            <Button mnemonicParsing="false" onMouseClicked="#handleMainMenu" text="main menu" />
        </items>
      </ToolBar>
   </children>
</AnchorPane>
